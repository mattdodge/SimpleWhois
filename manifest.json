{
  "manifest_version": 2,

  "name": "Simple WhoIs",
  "description": "An actual WhoIs Extension...not just a bookmarklet like all the others.",
  "version": "1.3",
  "icons": { 
	  "128": "whois.png",
	  "16": "whois16.png"
  },

  "permissions": [
      "tabs",
      "http://whothefuck.ru/"
  ],
  
  "browser_action": {
    "default_icon": "whois19.png",
    "default_popup": "popup.html"
  },
  
  "author" : "Matt Dodge",
  
  "omnibox" : {
	  "keyword" : "whois"
  },
  
  "background" : {
	  "scripts" : [
	      "background.js"
	  ]
  }
  
}